<style>
    body { font-family: Arial, sans-serif; }
</style>

<body>
<h1>Users</h1>

<ul>
  <% @users.each do |user| %>
      <li>
        <div>
          <a href="<%= user_path(user) %>">
          Employee ID: <%= user.Name %>
        </div> 
        <div>
          <%= link_to "Show details", user %>
        </div>
      </li>
  <% end %>
</ul>

<p>
  <%= link_to "New User", new_user_path %>
</p>

<p> 
<%= link_to "Go back", "/departments" %> 
</p>
</body>