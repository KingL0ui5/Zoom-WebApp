<html>
  <% provide(:title, "New Department") %> 
  <body>
    <div class ="container">
        <%= yield %> 
    </div>
    <div class="centre jumbotron">
      <h1>Create New Department</h1> <!-- from app/app/controllers/department_controller.rb -->
